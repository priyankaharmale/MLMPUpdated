                <android.support.design.widget.TextInputLayout
                    android:id="@+id/input_no_peple"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:errorTextAppearance="@style/hint_appearancered"
                    app:hintTextAppearance="@style/hint_appearanceblack">

                    <EditText
                        android:id="@+id/et_no_peple"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:backgroundTint="@color/colorBlack"
                        android:ems="10"
                        android:hint="@string/how_many_people_need_trained"
                        android:inputType="number"
                        android:singleLine="true"
                        android:textColor="@color/colorBlack"
                        android:textCursorDrawable="@drawable/cusrsor_black" />

                </android.support.design.widget.TextInputLayout>


                <android.support.design.widget.TextInputLayout
                    android:id="@+id/input_event_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:errorTextAppearance="@style/hint_appearancered"
                    app:hintTextAppearance="@style/hint_appearanceblack">

                    <EditText
                        android:id="@+id/et_event_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:backgroundTint="@color/colorBlack"
                        android:ems="10"
                        android:hint="@string/event_name"
                        android:singleLine="true"
                        android:textColor="@color/colorBlack"
                        android:textCursorDrawable="@drawable/cusrsor_black" />

                </android.support.design.widget.TextInputLayout>


